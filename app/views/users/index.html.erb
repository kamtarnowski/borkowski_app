<div class="container" id="back_other">

  <div class="row" id="row_next">
    <div class="col-md-offset-1 col-md-10">

      <div class="table-responsive">
        <table class="table table-hover">

          <tr>
            <th>Użytkownik_id</th>
            <th>Imię</th>
            <th>Email</th>
            <th>Usunięcie</th>
          </tr>
          <% @users.each do |user| %>
              <tr>
                <td><%= user.id %></td>
                <td><%= user.name %></td>
                <td><%= user.email %>
                <td><%= link_to '<i class="fa fa-times"></i>'.html_safe, admin_destroy_user_path(user), :method => :delete %></td>
              </tr>
          <% end %>

        </table>
      </div>

      <%= will_paginate @users, renderer: BootstrapPagination::Rails, :previous_label => t("Wstecz"), :next_label => t("Dalej") %>

    </div>
  </div>

</div>